<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Mesa" table="mesa">
    <id name="idmesa" type="integer" column="idmesa">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ncomensales" type="integer" column="ncomensales" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ubicacion" type="string" column="ubicacion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
