<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Cargo" table="cargo">
    <indexes>
      <index name="idx_descripcion" columns="descripcion"/>
    </indexes>
    <id name="idcargo" type="integer" column="idcargo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
